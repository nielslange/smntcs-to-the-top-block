{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "nielslange/to-the-top-block",
	"version": "0.1.0",
	"title": "To The Top Block",
	"category": "widgets",
	"icon": "arrow-up-alt",
	"description": "Adds a jump to the top link.",
	"supports": {
		"html": false,
		"align": false
	},
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "right"
		},
		"label": {
			"type": "string",
			"default": "Jump to top"
		}
	},
	"textdomain": "to-the-top-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
